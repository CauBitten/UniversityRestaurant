<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.gerente.ControllerPrincipalGerente">
   <children>
      <VBox alignment="CENTER" layoutX="134.0" layoutY="52.0" prefHeight="296.0" prefWidth="332.0" spacing="25.0">
         <children>
            <Button fx:id="buttonListarUsuarios" mnemonicParsing="false" onAction="#bttnListarUsuariosOn" prefHeight="44.0" prefWidth="332.0" style="-fx-background-color: #149959;" text="LISTAR USUARIOS" textFill="WHITE">
               <font>
                  <Font name="Georgia Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="buttonListarCardapios" mnemonicParsing="false" onAction="#bttnListarCardapiosOn" prefHeight="44.0" prefWidth="332.0" style="-fx-background-color: #149959;" text="LISTAR CARDAPIOS" textFill="WHITE">
               <font>
                  <Font name="Georgia Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="buttonListarRegistros" mnemonicParsing="false" onAction="#bttnListarRegistrosCompraOn" prefHeight="44.0" prefWidth="332.0" style="-fx-background-color: #149959;" text="LISTAR REGISTROS DE COMPRA" textFill="WHITE">
               <font>
                  <Font name="Georgia Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="buttonListarEntradas" mnemonicParsing="false" onAction="#bttnListarEntradasOn" prefHeight="44.0" prefWidth="332.0" style="-fx-background-color: #149959;" text="LISTAR ENTRADAS" textFill="WHITE">
               <font>
                  <Font name="Georgia Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="buttonCalendario" mnemonicParsing="false" onAction="#bttnCalendario" prefHeight="44.0" prefWidth="332.0" style="-fx-background-color: #149959;" text="VER CALENDARIO REGISTRADO" textFill="WHITE">
               <font>
                  <Font name="Georgia Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Hyperlink fx:id="hlSair" layoutX="554.0" layoutY="14.0" onAction="#hlSairOn" text="Sair" textFill="#149959">
         <font>
            <Font name="Georgia Italic" size="12.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
