<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.gerente.ControllerListarEntradas">
   <children>
      <Button fx:id="buttonVoltarPagina" layoutX="24.0" layoutY="24.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#bttnVoltarPaginaOn" prefHeight="25.0" prefWidth="25.0" style="-fx-background-color: #149959;" text="&lt;" textFill="WHITE">
         <font>
            <Font name="Georgia Bold" size="12.0" />
         </font>
      </Button>
      <VBox layoutX="61.0" layoutY="50.0" prefHeight="271.0" prefWidth="478.0">
         <children>
            <TableView prefHeight="276.0" prefWidth="478.0">
              <columns>
                <TableColumn fx:id="tblColTipo" prefWidth="75.0" text="Tipo" />
                <TableColumn fx:id="tblColDataHora" prefWidth="75.0" text="Data/Hora" />
                  <TableColumn fx:id="tblColIdCardapio" prefWidth="75.0" text="Cardapio ID" />
                  <TableColumn fx:id="tblColIdFicha" prefWidth="75.0" text="Ficha ID" />
                  <TableColumn fx:id="tblColCliente" prefWidth="75.0" text="Cliente" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="bttnFiltrar" layoutX="233.0" layoutY="337.0" mnemonicParsing="false" onAction="#bttnFiltrarOn" prefHeight="35.0" prefWidth="134.0" style="-fx-background-color: #149959;" text="FILTRAR" textFill="WHITE">
         <font>
            <Font name="Georgia Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
